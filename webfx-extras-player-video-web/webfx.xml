<?xml version="1.0" encoding="UTF-8"?>
<project>

    <target-tags>
        <!-- in this context web doesn't refer to the webfx platform target -->
        <ignore-tag>web</ignore-tag>
    </target-tags>

    <exported-packages>
        <source-packages/>
        <resource-packages/>
    </exported-packages>

    <dependencies>
        <used-by-source-modules/>
    </dependencies>

    <java-callbacks>
        <callback-class name="dev.webfx.extras.player.video.web.SeamlessCapableWebVideoPlayer">
            <callback-method name="onReady"/>
            <callback-method name="onPlay"/>
            <callback-method name="onPause"/>
            <callback-method name="onEnd"/>
        </callback-class>
    </java-callbacks>

</project>